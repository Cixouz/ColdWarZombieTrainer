<Window x:Class="ColdWarZombieTrainer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ColdWarZombieTrainer"
        mc:Ignorable="d"
        Title="Tranny Trainer" Height="450" Width="815"
        ResizeMode="NoResize">
    <Grid Background="#FF191919">
        <Label Content="Tranny Trainer" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" FontSize="25" FontFamily="Bahnschrift Light"/>
        <Label Content="Console" HorizontalAlignment="Left" Margin="10,233,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.678,1.548"/>

        <CheckBox Content="God Mode" Name="GodModeCheckBox" HorizontalAlignment="Left" Margin="10,35,0,0" VerticalAlignment="Top" Checked="GodModeEnable" Unchecked="GodModeDisable"/>

        <Button Content="Start" HorizontalAlignment="Left" Margin="725,13,0,0" VerticalAlignment="Top" Height="35" Width="65" Name="StartButton" Click="StartButton_Click"/>
        <Button Content="Test" HorizontalAlignment="Left" Margin="725,217,0,0" VerticalAlignment="Top" Height="35" Width="65" Name="TestButton" Click="TestButton_Click"/>

        <TextBox Name="Console" HorizontalAlignment="Center" Margin="0,264,0,0" VerticalAlignment="Top" Height="160" Width="780" Background="#FF2D2D2D" IsEnabled="False"/>
    </Grid>
</Window>
